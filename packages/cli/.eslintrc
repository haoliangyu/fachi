{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "space-before-function-paren": ["warn", "always"],
    "interface-name-prefix": "always",
    "arrow-parens": [
      "warn",
      "always"
    ],
    "@typescript-eslint/no-empty-function": "off"
  }
}
